{
  "name": "@kendraio/console",
  "description": "Kendraio Console",
  "version": "0.0.2",
  "author": "Kendraio @kendraio",
  "bin": {
    "kendraio": "./bin/run"
  },
  "bugs": "https://github.com/kendraio/kendraio-console/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "fs-extra": "^7.0.1",
    "json-2-csv": "^3.4.1",
    "lodash-es": "^4.17.11",
    "tslib": "^1",
    "x2js": "^3.2.6"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/fs-extra": "^5.0.5",
    "@types/lodash-es": "^4.17.3",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "globby": "^8",
    "lodash": "^4.17.11",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kendraio/kendraio-console",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "kendraio",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kendraio/kendraio-console",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p . --noEmit && tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
