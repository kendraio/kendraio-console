{
  "extends": "@oclif/tslint",
  "rules": {
    "semicolon": true,
    "no-console": false,
    "no-trailing-whitespace": false,
    "no-consecutive-blank-lines": false,
    "object-curly-spacing": true,
    "no-string-literal": false,
    "no-empty-line-after-opening-brace": false,
    "no-http-string": false,
    "no-unused": false
  }
}
